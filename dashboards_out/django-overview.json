{
   "__inputs": [ ],
   "__requires": [ ],
   "annotations": {
      "list": [ ]
   },
   "description": "A dashboard that monitors Django which focuses on giving a overview for the system (requests, db, cache). The dashboards were generated using [Django-mixin](https://github.com/adinhodovic/django-mixin). Open issues and create feature requests in the repository.",
   "editable": false,
   "links": [
      {
         "asDropdown": true,
         "includeVars": true,
         "keepTime": true,
         "tags": [
            "django",
            "django-mixin"
         ],
         "targetBlank": true,
         "title": "Django",
         "type": "dashboards"
      }
   ],
   "panels": [
      {
         "collapsed": false,
         "gridPos": {
            "h": 1,
            "w": 24,
            "x": 0,
            "y": 0
         },
         "id": 1,
         "title": "Summary",
         "type": "row"
      },
      {
         "datasource": {
            "type": "prometheus",
            "uid": "$datasource"
         },
         "description": "The number of requests received per second.",
         "fieldConfig": {
            "defaults": {
               "mappings": [ ],
               "thresholds": {
                  "steps": [
                     {
                        "color": "red",
                        "value": 0
                     },
                     {
                        "color": "green",
                        "value": 0.10000000000000001
                     }
                  ]
               },
               "unit": "reqps"
            }
         },
         "gridPos": {
            "h": 4,
            "w": 8,
            "x": 0,
            "y": 1
         },
         "id": 2,
         "pluginVersion": "v11.4.0",
         "targets": [
            {
               "datasource": {
                  "type": "prometheus",
                  "uid": "${datasource}"
               },
               "expr": "round(\n  sum(\n    rate(\n      django_http_requests_total_by_view_transport_method_total{\n        cluster=\"$cluster\",\nnamespace=\"$namespace\",\njob=~\"$job\"\n,\nview!~\"<unnamed view>|health_check:health_check_home|prometheus-django-metrics\"\n\n      }[$__rate_interval]\n    )\n  ), 0.001\n)\n"
            }
         ],
         "title": "Request Volume",
         "type": "stat"
      },
      {
         "datasource": {
            "type": "prometheus",
            "uid": "$datasource"
         },
         "description": "The number of database operations (queries) executed per second.",
         "fieldConfig": {
            "defaults": {
               "mappings": [ ],
               "thresholds": {
                  "steps": [
                     {
                        "color": "red",
                        "value": 0
                     },
                     {
                        "color": "green",
                        "value": 0.10000000000000001
                     }
                  ]
               },
               "unit": "ops"
            }
         },
         "gridPos": {
            "h": 4,
            "w": 8,
            "x": 8,
            "y": 1
         },
         "id": 3,
         "pluginVersion": "v11.4.0",
         "targets": [
            {
               "datasource": {
                  "type": "prometheus",
                  "uid": "${datasource}"
               },
               "expr": "sum (\n  rate (\n    django_db_execute_total {\n      cluster=\"$cluster\",\nnamespace=\"$namespace\",\njob=~\"$job\"\n\n    }[$__rate_interval]\n  )\n) by (namespace, job)\n"
            }
         ],
         "title": "Database Ops",
         "type": "stat"
      },
      {
         "datasource": {
            "type": "prometheus",
            "uid": "$datasource"
         },
         "description": "The ratio of cache hits to total cache requests over the last 30 minutes. A higher hit rate indicates better cache performance.",
         "fieldConfig": {
            "defaults": {
               "mappings": [ ],
               "thresholds": {
                  "steps": [
                     {
                        "color": "red",
                        "value": 0
                     },
                     {
                        "color": "green",
                        "value": 0.10000000000000001
                     }
                  ]
               },
               "unit": "percentunit"
            }
         },
         "gridPos": {
            "h": 4,
            "w": 8,
            "x": 16,
            "y": 1
         },
         "id": 4,
         "pluginVersion": "v11.4.0",
         "targets": [
            {
               "datasource": {
                  "type": "prometheus",
                  "uid": "${datasource}"
               },
               "expr": "sum (\n  rate (\n    django_cache_get_hits_total {\n      cluster=\"$cluster\",\nnamespace=\"$namespace\",\njob=~\"$job\"\n\n    }[30m]\n  )\n) by (namespace, job)\n/\nsum (\n  rate (\n    django_cache_get_total {\n      cluster=\"$cluster\",\nnamespace=\"$namespace\",\njob=~\"$job\"\n\n    }[30m]\n  )\n) by (namespace, job)\n"
            }
         ],
         "title": "Cache Hitrate [30m]",
         "type": "stat"
      },
      {
         "datasource": {
            "type": "prometheus",
            "uid": "$datasource"
         },
         "description": "The number of HTTP responses sent per second, categorized by status code classes.",
         "fieldConfig": {
            "defaults": {
               "custom": {
                  "fillOpacity": 100,
                  "lineWidth": 1,
                  "stacking": {
                     "mode": "percent"
                  }
               },
               "unit": "reqps"
            },
            "overrides": [
               {
                  "matcher": {
                     "id": "byName",
                     "options": "2xx"
                  },
                  "properties": [
                     {
                        "id": "color",
                        "value": {
                           "fixedColor": "green",
                           "mode": "fixed"
                        }
                     }
                  ]
               },
               {
                  "matcher": {
                     "id": "byName",
                     "options": "3xx"
                  },
                  "properties": [
                     {
                        "id": "color",
                        "value": {
                           "fixedColor": "blue",
                           "mode": "fixed"
                        }
                     }
                  ]
               },
               {
                  "matcher": {
                     "id": "byName",
                     "options": "4xx"
                  },
                  "properties": [
                     {
                        "id": "color",
                        "value": {
                           "fixedColor": "yellow",
                           "mode": "fixed"
                        }
                     }
                  ]
               },
               {
                  "matcher": {
                     "id": "byName",
                     "options": "5xx"
                  },
                  "properties": [
                     {
                        "id": "color",
                        "value": {
                           "fixedColor": "red",
                           "mode": "fixed"
                        }
                     }
                  ]
               }
            ]
         },
         "gridPos": {
            "h": 6,
            "w": 24,
            "x": 0,
            "y": 5
         },
         "id": 5,
         "options": {
            "legend": {
               "calcs": [
                  "mean",
                  "max"
               ],
               "displayMode": "table",
               "placement": "right",
               "showLegend": true,
               "sortBy": "Mean",
               "sortDesc": true
            },
            "tooltip": {
               "mode": "multi",
               "sort": "desc"
            }
         },
         "pluginVersion": "v11.4.0",
         "targets": [
            {
               "datasource": {
                  "type": "prometheus",
                  "uid": "$datasource"
               },
               "exemplar": false,
               "expr": "round(\n  sum(\n    rate(\n      django_http_responses_total_by_status_view_method_total{\n        cluster=\"$cluster\",\nnamespace=\"$namespace\",\njob=~\"$job\"\n,\nview!~\"<unnamed view>|health_check:health_check_home|prometheus-django-metrics\"\n,\n        status=~\"2.*\",\n      }[$__rate_interval]\n    ) > 0\n  ) by (job), 0.001\n)\n",
               "legendFormat": "2xx"
            },
            {
               "datasource": {
                  "type": "prometheus",
                  "uid": "$datasource"
               },
               "exemplar": false,
               "expr": "round(\n  sum(\n    rate(\n      django_http_responses_total_by_status_view_method_total{\n        cluster=\"$cluster\",\nnamespace=\"$namespace\",\njob=~\"$job\"\n,\nview!~\"<unnamed view>|health_check:health_check_home|prometheus-django-metrics\"\n,\n        status=~\"3.*\",\n      }[$__rate_interval]\n    ) > 0\n  ) by (job), 0.001\n)\n",
               "legendFormat": "3xx"
            },
            {
               "datasource": {
                  "type": "prometheus",
                  "uid": "$datasource"
               },
               "exemplar": false,
               "expr": "round(\n  sum(\n    rate(\n      django_http_responses_total_by_status_view_method_total{\n        cluster=\"$cluster\",\nnamespace=\"$namespace\",\njob=~\"$job\"\n,\nview!~\"<unnamed view>|health_check:health_check_home|prometheus-django-metrics\"\n,\n        status=~\"4.*\",\n      }[$__rate_interval]\n    ) > 0\n  ) by (job), 0.001\n)\n",
               "legendFormat": "4xx"
            },
            {
               "datasource": {
                  "type": "prometheus",
                  "uid": "$datasource"
               },
               "exemplar": false,
               "expr": "round(\n  sum(\n    rate(\n      django_http_responses_total_by_status_view_method_total{\n        cluster=\"$cluster\",\nnamespace=\"$namespace\",\njob=~\"$job\"\n,\nview!~\"<unnamed view>|health_check:health_check_home|prometheus-django-metrics\"\n,\n        status=~\"5.*\",\n      }[$__rate_interval]\n    ) > 0\n  ) by (job), 0.001\n)\n",
               "legendFormat": "5xx"
            }
         ],
         "title": "Responses",
         "type": "timeseries"
      },
      {
         "collapsed": false,
         "gridPos": {
            "h": 1,
            "w": 24,
            "x": 0,
            "y": 11
         },
         "id": 6,
         "title": "Database",
         "type": "row"
      },
      {
         "datasource": {
            "type": "prometheus",
            "uid": "$datasource"
         },
         "description": "The total number of database migrations that have been applied.",
         "fieldConfig": {
            "defaults": {
               "mappings": [ ],
               "thresholds": {
                  "steps": [
                     {
                        "color": "green",
                        "value": 0
                     }
                  ]
               },
               "unit": "short"
            }
         },
         "gridPos": {
            "h": 3,
            "w": 12,
            "x": 0,
            "y": 12
         },
         "id": 7,
         "pluginVersion": "v11.4.0",
         "targets": [
            {
               "datasource": {
                  "type": "prometheus",
                  "uid": "${datasource}"
               },
               "expr": "max (\n  django_migrations_applied_total {\n    cluster=\"$cluster\",\nnamespace=\"$namespace\",\njob=~\"$job\"\n\n  }\n) by (namespace, job)\n"
            }
         ],
         "title": "Migrations Applied",
         "type": "stat"
      },
      {
         "datasource": {
            "type": "prometheus",
            "uid": "$datasource"
         },
         "description": "The total number of database migrations that are pending and have not yet been applied.",
         "fieldConfig": {
            "defaults": {
               "mappings": [ ],
               "thresholds": {
                  "steps": [
                     {
                        "color": "green",
                        "value": 0
                     },
                     {
                        "color": "red",
                        "value": 0.10000000000000001
                     }
                  ]
               },
               "unit": "short"
            }
         },
         "gridPos": {
            "h": 3,
            "w": 12,
            "x": 12,
            "y": 12
         },
         "id": 8,
         "pluginVersion": "v11.4.0",
         "targets": [
            {
               "datasource": {
                  "type": "prometheus",
                  "uid": "${datasource}"
               },
               "expr": "max (\n  django_migrations_unapplied_total {\n    cluster=\"$cluster\",\nnamespace=\"$namespace\",\njob=~\"$job\"\n\n  }\n) by (namespace, job)\n"
            }
         ],
         "title": "Migrations Unapplied",
         "type": "stat"
      },
      {
         "datasource": {
            "type": "prometheus",
            "uid": "$datasource"
         },
         "description": "The number of new database connections established, grouped by database vendor. Monitoring connection trends can help identify potential bottlenecks or capacity issues.",
         "fieldConfig": {
            "defaults": {
               "custom": {
                  "fillOpacity": 10
               },
               "unit": "short"
            },
            "overrides": [ ]
         },
         "gridPos": {
            "h": 5,
            "w": 12,
            "x": 0,
            "y": 15
         },
         "id": 9,
         "options": {
            "legend": {
               "calcs": [
                  "mean",
                  "max"
               ],
               "displayMode": "table",
               "placement": "right",
               "showLegend": true,
               "sortBy": "Mean",
               "sortDesc": true
            },
            "tooltip": {
               "mode": "multi",
               "sort": "desc"
            }
         },
         "pluginVersion": "v11.4.0",
         "targets": [
            {
               "datasource": {
                  "type": "prometheus",
                  "uid": "$datasource"
               },
               "exemplar": false,
               "expr": "round(\n  sum(\n    increase(\n      django_db_new_connections_total{\n        cluster=\"$cluster\",\nnamespace=\"$namespace\",\njob=~\"$job\"\n\n      }[$__rate_interval]\n    ) > 0\n  ) by (namespace, job, vendor)\n)\n",
               "legendFormat": "{{ vendor }}"
            }
         ],
         "title": "Database Connections",
         "type": "timeseries"
      },
      {
         "datasource": {
            "type": "prometheus",
            "uid": "$datasource"
         },
         "description": "The latency of database queries at various percentiles, grouped by database vendor. This helps identify performance issues and outliers in database response times.",
         "fieldConfig": {
            "defaults": {
               "custom": {
                  "fillOpacity": 10
               },
               "unit": "s"
            },
            "overrides": [ ]
         },
         "gridPos": {
            "h": 5,
            "w": 12,
            "x": 12,
            "y": 15
         },
         "id": 10,
         "options": {
            "legend": {
               "calcs": [
                  "mean",
                  "max"
               ],
               "displayMode": "table",
               "placement": "right",
               "showLegend": true,
               "sortBy": "Mean",
               "sortDesc": true
            },
            "tooltip": {
               "mode": "multi",
               "sort": "desc"
            }
         },
         "pluginVersion": "v11.4.0",
         "targets": [
            {
               "datasource": {
                  "type": "prometheus",
                  "uid": "$datasource"
               },
               "exemplar": false,
               "expr": "histogram_quantile(0.50,\n  sum(\n    irate(\n      django_db_query_duration_seconds_bucket{\n        cluster=\"$cluster\",\nnamespace=\"$namespace\",\njob=~\"$job\"\n\n      }[$__rate_interval]\n    ) > 0\n  ) by (vendor, namespace, job, le)\n)\n",
               "legendFormat": "50 - {{ vendor }}"
            },
            {
               "datasource": {
                  "type": "prometheus",
                  "uid": "$datasource"
               },
               "exemplar": false,
               "expr": "histogram_quantile(0.95,\n  sum(\n    irate(\n      django_db_query_duration_seconds_bucket{\n        cluster=\"$cluster\",\nnamespace=\"$namespace\",\njob=~\"$job\"\n\n      }[$__rate_interval]\n    ) > 0\n  ) by (vendor, namespace, job, le)\n)\n",
               "legendFormat": "95 - {{ vendor }}"
            },
            {
               "datasource": {
                  "type": "prometheus",
                  "uid": "$datasource"
               },
               "exemplar": false,
               "expr": "histogram_quantile(0.99,\n  sum(\n    irate(\n      django_db_query_duration_seconds_bucket{\n        cluster=\"$cluster\",\nnamespace=\"$namespace\",\njob=~\"$job\"\n\n      }[$__rate_interval]\n    ) > 0\n  ) by (vendor, namespace, job, le)\n)\n",
               "legendFormat": "99 - {{ vendor }}"
            },
            {
               "datasource": {
                  "type": "prometheus",
                  "uid": "$datasource"
               },
               "exemplar": false,
               "expr": "histogram_quantile(0.999,\n  sum(\n    irate(\n      django_db_query_duration_seconds_bucket{\n        cluster=\"$cluster\",\nnamespace=\"$namespace\",\njob=~\"$job\"\n\n      }[$__rate_interval]\n    ) > 0\n  ) by (vendor, namespace, job, le)\n)\n",
               "legendFormat": "99.9 - {{ vendor }}"
            }
         ],
         "title": "Database Latency",
         "type": "timeseries"
      },
      {
         "datasource": {
            "type": "prometheus",
            "uid": "$datasource"
         },
         "description": "A table displaying the top 10 most frequent database error types over the past week. This helps identify recurring issues that may need attention.",
         "fieldConfig": {
            "defaults": {
               "unit": "short"
            },
            "overrides": [ ]
         },
         "gridPos": {
            "h": 8,
            "w": 24,
            "x": 0,
            "y": 20
         },
         "id": 11,
         "options": {
            "footer": {
               "enablePagination": true
            },
            "sortBy": [
               {
                  "desc": true,
                  "displayName": "Type"
               }
            ]
         },
         "pluginVersion": "v11.4.0",
         "targets": [
            {
               "datasource": {
                  "type": "prometheus",
                  "uid": "$datasource"
               },
               "expr": "round(\n  topk(10,\n    sum(\n      increase(\n        django_db_errors_total{\n          cluster=\"$cluster\",\nnamespace=\"$namespace\",\njob=~\"$job\"\n\n        }[1w]\n      ) > 0\n    ) by (type)\n  )\n)\n",
               "format": "table",
               "instant": true
            }
         ],
         "title": "Top Database Errors (1w)",
         "transformations": [
            {
               "id": "organize",
               "options": {
                  "excludeByName": {
                     "Time": true
                  },
                  "indexByName": {
                     "job": 1,
                     "namespace": 0,
                     "type": 2
                  },
                  "renameByName": {
                     "job": "Job",
                     "namespace": "Namespace",
                     "type": "Type"
                  }
               }
            }
         ],
         "type": "table"
      },
      {
         "collapsed": false,
         "gridPos": {
            "h": 1,
            "w": 24,
            "x": 0,
            "y": 28
         },
         "id": 12,
         "title": "Cache",
         "type": "row"
      },
      {
         "datasource": {
            "type": "prometheus",
            "uid": "$datasource"
         },
         "description": "The number of cache get operations, categorized by hits and misses, grouped by cache backend. This helps assess cache performance and effectiveness.",
         "fieldConfig": {
            "defaults": {
               "custom": {
                  "fillOpacity": 100,
                  "lineWidth": 1,
                  "stacking": {
                     "mode": "percent"
                  }
               },
               "unit": "ops"
            },
            "overrides": [ ]
         },
         "gridPos": {
            "h": 6,
            "w": 24,
            "x": 0,
            "y": 29
         },
         "id": 13,
         "options": {
            "legend": {
               "calcs": [
                  "mean",
                  "max"
               ],
               "displayMode": "table",
               "placement": "right",
               "showLegend": true,
               "sortBy": "Mean",
               "sortDesc": true
            },
            "tooltip": {
               "mode": "multi",
               "sort": "desc"
            }
         },
         "pluginVersion": "v11.4.0",
         "targets": [
            {
               "datasource": {
                  "type": "prometheus",
                  "uid": "$datasource"
               },
               "exemplar": false,
               "expr": "sum(\n  rate(\n    django_cache_get_hits_total{\n      cluster=\"$cluster\",\nnamespace=\"$namespace\",\njob=~\"$job\"\n\n    }[$__rate_interval]\n  ) > 0\n) by (namespace, job, backend)\n",
               "legendFormat": "Hit - {{ backend }}"
            },
            {
               "datasource": {
                  "type": "prometheus",
                  "uid": "$datasource"
               },
               "exemplar": false,
               "expr": "sum(\n  rate(\n    django_cache_get_misses_total{\n      cluster=\"$cluster\",\nnamespace=\"$namespace\",\njob=~\"$job\"\n\n    }[$__rate_interval]\n  ) > 0\n) by (namespace, job, backend)\n",
               "legendFormat": "Miss - {{ backend }}"
            }
         ],
         "title": "Cache Get Operations",
         "type": "timeseries"
      }
   ],
   "schemaVersion": 39,
   "tags": [
      "django",
      "django-mixin"
   ],
   "templating": {
      "list": [
         {
            "current": {
               "selected": true,
               "text": "default",
